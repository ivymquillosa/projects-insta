                  p.padd-left2 !{data.lbl_make_model_of_device_repaired.ja}
                  p.padd-left2 !{data.lbl_imei_SIMPLE.ja}
                  p.padd-left2 !{data.lbl_type_of_repair_screen_SIMPLE.ja}
                  p.padd-left2 !{data.lbl_total_cost_of_repair_SIMPLE.ja}
                  p.padd-left2 !{data.lbl_date_of_repair_after_claim_filed_SIMPLE.ja}

                  p.padd-left2 !{data.lbl_you_will_be_reimbursed.ja}
                  p.padd-left !{data.lbl_once_the_Claim_has_been_completed.ja}

