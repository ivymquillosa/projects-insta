extends layoutterms

block content
    .section.section__documenthero
      .section--wrapper
        .bgimage.antimoderate.antimoderate-bg(data-hdsrc='/img/getstarted_bg.webp' alt='Instaprotek Background', data-antimoderate-scale='1', data-ldsrc='/img/getstarted_bg_alt.webp', classnames='bgimage')

    .section__group.section__group--offset.bg--darkgray

      section.section.section__documents
        .section--wrapper
          .row
            .col.s12.m12
              .step__doc.anim(data-anim='fadeInDown', data-delay='0s', data-duration='1s')
                h1.step__doc__label #{data.lbl_terms_and_condition.ja}
                .step__doc__content
                  h3.main-title #{data.lbl_limited_product_guarantee.ja}
                  p !{data.lbl_LIQUIPEL_PROTECTION_LLC.ja}

                  //- 1. Program Overview.
                  p !{data.lbl_program_overview_new.ja}
                  p !{data.lbl_your_authentic.ja}
                  p.padd-left !{data.lbl_eligibility.ja}
                  p.padd-left !{data.lbl_registration_new.ja}
                  p.padd-left !{data.lbl_how_to_file_a_claim.ja}

                  //- 2. Device Protection Limited Guarantee.
                  p !{data.lbl_device_protection_guarantee.ja}
                  p !{data.lbl_whats_covered.ja}
                  p !{data.lbl_whats_excluded.ja}
                  p !{data.lbl_your_eligible_device.ja}

                  //- 3. Eligibility Requirements.
                  p !{data.lbl_eligibility_requirements_new.ja}
                  p.padd-left !{data.lbl_program_eligibility_is_conditioned_new.ja}
                  p.padd-left2 !{data.lbl_you_must_be_an_individual_consumer.ja}
                  p.padd-left2 !{data.lbl_you_must_be_a_valid_resident.ja}
                  p.padd-left2 !{data.lbl_you_must_have_purchased.ja}
                  p.padd-left2 !{data.lbl_you_must_have_validly_registered.ja}
                  p.padd-left2 !{data.lbl_you_must_submit_a_claim_new.ja}
                  p.padd-left2 !{data.lbl_the_eligible_device.ja}

                  //- 4 Registration Process.
                  p !{data.lbl_registration_process_new.ja}
                  p !{data.lbl_as_part_of_the_eligibility_requirements_new.ja}
                  p.padd-left !{data.lbl_within_the_registration_period_new.ja}
                  p.padd-left !{data.lbl_using_your_eligible_device.ja}
                  p.padd-left2 !{data.lbl_your_original_dated_proof.ja}
                  p.padd-left2 !{data.lbl_the_imei_number_for_the_eligible_device.ja}
                  p.padd-left2 !{data.lbl_any_pertinent_personal_information_new.ja}
                  p.padd-left2 !{data.lbl_for_a_complete_description_of_how.ja}

                  p.padd-left !{data.lbl_you_must_successfully_pass.ja}
                  p.padd-left !{data.lbl_after_successfully_completing.ja}
                  p.padd-left !{data.lbl_once_you_are_registered_new.ja}

                  //- 5 Claim Submissions.
                  p !{data.lbl_claim_submissions.ja}
                  p !{data.lbl_submit_a_Claim_as_follows.ja}
                  p.padd-left !{data.lbl_initiate_a_claim_through.ja}
                  p.padd-left !{data.lbl_your_claim_must_be_verified_new.ja}

                  p.padd-left2 !{data.lbl_name_of_person_submitting_claim.ja}
                  p.padd-left2 !{data.lbl_the_SIMPLE_product_was_registered.ja}
                  p.padd-left2 !{data.lbl_all_required_information.ja}
                  p.padd-left2 !{data.lbl_the_SIMPLE_product_is_authentic.ja}
                  p.padd-left2 !{data.lbl_your_device_is_an_eligible_device.ja}

                  p.padd-left !{data.lbl_you_will_be_required_to_submit.ja}
                  p.padd-left !{data.lbl_if_your_claim_passes_the_verification_process_new.ja}
                  p.padd-left !{data.lbl_you_will_have_thirty.ja}

                  p.padd-left2 !{data.lbl_repair_provider_name.ja}
