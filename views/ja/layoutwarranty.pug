  body.sticky--header

    //- #main-preloader.preloader
    //-   svg.preloader__spinner(width='120px' height='120px' viewbox='0 0 130 130' xmlns='http://www.w3.org/2000/svg')
    //-     circle.preloader__path(fill='none' stroke-width='10' stroke-linecap='round' cx='65' cy='65' r='60')
    //-   img.preloader__logo(src='/img/preloader.webp' alt='Instaprotek preloader')

    #app-container
      .page__home
        
        block nav
          header.main__header
            .main__header--wrapper
              .branding
                a(href='/ja')
                  img.branding__image.light(src='/img/logo_full_light.webp' alt='Insaprotek logo light')
                  img.branding__image.dark(src='/img/logo_full_dark.webp' alt='Insaprotek logo dark')
            .main__nav
              a.responsive.dropdown-trigger(href='#' data-target="languages" id='country')
                img.lang__flag(src="/img/icons/flag_japan.webp" alt='dropdown menu USA flag' width='25px' height='18px')
                i.material-icons keyboard_arrow_down
              ul.language-list.dropdown-content(id='languages')
                li.language-list-item(data='en')
                  a(href='/en')
                    img.lang__flag(src="/img/icons/flag_us.webp" alt='dropdown menu USA flag' width='25px' height='18px')
                    label English
                li.language-list-item(data='fr')
                  a(href='/fr')
                    img.lang__flag(src="/img/icons/flag_france.webp" alt='dropdown menu France flag' width='25px' height='18px')
                    label French
                li.language-list-item(data='ja')
                  a(href='/ja')
                    img.lang__flag(src="/img/icons/flag_japan.webp" alt='dropdown menu Japan flag' width='25px' height='18px')
                    label Japanese
                //- li.language-list-item(data='es')
                //-   a(href='/es')
                //-     img.lang__flag(src="/imgs/icons/flag_spain.png" alt='dropdown menu Spain flag' width='25px' height='18px')
                //-     label Spanish
              a.main__nav--toggle(href='#menu-toggle')
                span
                span
                span
              .list.list--horizontal.lang_responsive
                .list__item
                  a(href='/ja/support') #{data.menu_support.ja}
                .list__item
                  a(href='/ja/contact') #{data.menu_contact_us.ja}
                //- .list__item
                //-   a(href='/ja/warranty') #{data.menu_warranty_program.ja}

        .page--wrapper
          block content

          block footer
            footer.footer
              .container
                .row
                  .col.s12.m12
                    p.text--small.footer__text
                      | #{data.lbl_instaprotek_copyright.ja}
                  .col.s12.m12
                    ul.footer__list.list.list--horizontal
                      li.list__item
                        a(href='/ja/privacy') #{data.menu_privacy_statement.ja}
                      li.list__item
                        a(href='/ja/terms') #{data.menu_terms_and_condition.ja}
                      li.list__item
                        a(href='/ja/support') #{data.menu_support.ja}
                      li.list__item
                        a(href='/ja/contact') #{data.menu_contact_us.ja}

