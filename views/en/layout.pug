        #modalGetStarted.modal.modal__getstarted.modal-fixed-footer
          .modal-header
            p #{data.lbl_get_started.en}
          .modal-content
            .modal-content--wrapper
              .row
                form.col.s12.form__getstarted.form.formjs
                  input(type='hidden', id='form_targetname', value='InstaProtek Support')
                  input(type='hidden', id='form_targetemail', value='support@instaprotek.com')
                  input(type='hidden', id='form_emailtitle', value='InstaProtek Get Started')
                  .form__process
                    .spinner__wrapper
                      span.spinner
                      span.spinner__text #{data.lbl_processing.en}
                  .form__container.form__container--start
                    .row.nomargin
                      .col.s12
                        p #{data.lbl_please_fill_up_this_form.en}
                    .row.nomargin
                      .input-field.inline.col.s6
                        i.material-icons.prefix account_circle
                        input#form_first_name.input.validate(type='text', required, inputtype='alphabet', data-placeholder=data.txt_first_name.en)
                        label(for='form_first_name') #{data.txt_first_name.en}
                        span.helper-text(data-error='Required.')
                      .input-field.inline.col.s6
                        input#form_last_name.input.validate(type='text', required, inputtype='alphabet', data-placeholder=data.txt_last_name.en)
                        label(for='form_last_name') #{data.txt_last_name.en}
                        span.helper-text(data-error='Required.')
                      .input-field.inline.col.s12
                        i.material-icons.prefix account_balance
                        input#form_company_name.input.validate(type='text', required, inputtype='alphabet', data-placeholder=data.txt_company_name.en)
                        label(for='form_company_name') #{data.txt_company_name.en}
                        span.helper-text(data-error='Required.')
                      .input-field.inline.col.s12
                        i.material-icons.prefix email
                        input#form_email_address.input.validate(type='email', required, inputtype='email', data-placeholder=data.txt_email_address.en)
                        label(for='form_email_address') #{data.txt_email_address.en}
                        span.helper-text(data-error='Please provide a valid emaill address.')
                      .input-field.inline.col.s12
                        i.material-icons.prefix phone
                        input#form_phone_number.input.validate(type='text', required, inputtype='phone', data-placeholder=data.txt_phone_number.en)
                        label(for='form_phone_number') #{data.txt_phone_number.en}
                        span.helper-text(data-error='Required.')
                      .input-field.inline.col.s12
                        i.material-icons.prefix message
                        textarea#form_message.input.materialize-textarea(required, inputtype='alphabet', data-placeholder=data.txt_message.en)
                        label(for='form_message') #{data.txt_message.en}
                        span.helper-text(data-error='Required.')
                  .form__container.form__container--done
                    .row
                      .col.s12
                        h2 #{data.lbl_thank_you_for_interest.en}
                        h3 #{data.lbl_please_allow_24_hours_for_us_to_process.en}
          .modal-footer
            .modal-footer--left
              a.form-reset.waves-effect.waves-default.btn.btn-flat(href='#!') #{data.btn_reset.en}
            .modal-footer--right
              a.form-cancel.modal-close.waves-effect.waves-default.btn.btn-flat(href='#!') #{data.btn_cancel.en}
              a.form-close.modal-close.waves-effect.waves-red.btn.btn-flat(href='#!') #{data.btn_close.en}
              a.form-submit.waves-effect.waves-light.btn(href='#!') #{data.btn_submit.en}

