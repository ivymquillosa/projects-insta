                    li !{data.lbl_identifiers_like_your_name_mailing_address_telephone_number.en}
                    li !{data.lbl_commercial_information_including_your_registration_status.en}
                    li !{data.lbl_internet_activity_like_your_browsing_history.en}
                    li !{data.lbl_location_data_like_your_street_address_city_zip_code.en}
                    li !{data.lbl_professional_or_employment_related_information_like_your_occupation.en}
                    li !{data.lbl_other_personal_information_that_you_voluntarily.en}
                    li !{data.lbl_inferences_drawn_from_the_personal_information_we_collect.en}
                  
