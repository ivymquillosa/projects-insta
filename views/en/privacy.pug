          .row
            .col.s12.m12
              .step__doc.anim(data-anim='fadeInDown', data-delay='0s', data-duration='1s')
                h1.step__doc__label #{data.lbl_privacy_policy.en}
                .step__doc__content.privacy-top
                  //- p #{data.lbl_instprotek_is_commited_to_the_protection_and_proper_use_of_our_customers_personal_information.en}
                  //- p !{data.lbl_if_you_have_any_questions_or_concerns.en}

                  //- h3 #{data.lbl_personal_information_we_collect.en}

                  //- p #{data.lbl_we_may_collect_personally_identifiable_information.en}                  
                  //- p #{data.lbl_we_may_also_collect_certain_additional_information.en}
                  //- h3 #{data.lbl_how_we_use_and_disclose_your_personal_information.en}
                  //- p #{data.lbl_we_use_your_personal_information_to_conduct_our_business.en}
                  //- ul
                  //-   li #{data.lbl_we_may_use_your_personal_information_to_help_us_develop.en}
                  //-   li #{data.lbl_we_may_use_your_personal_information_to_contact_you_regarding_upcoming_events.en}
