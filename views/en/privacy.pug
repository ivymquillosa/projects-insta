                    li !{data.lbl_sell_your_personal_information.en}
                    li !{data.lbl_process_your_personal_information_for.en}
                    li !{data.lbl_retain_use_or_disclose_your_personal.en}
                  p #{data.lbl_for_the_avoidance_of_doubt_we_are_not_sharing.en}

                  p(style='text-align: center;') <u>!{data.lbl_your_rights_and_choices.en}</u>
                  p #{data.lbl_the_california_consumer_privacy_act.en}
                  ul
                    li !{data.lbl_right_to_know_and_access.en}
                    li !{data.lbl_right_to_request_deletion.en}
                    li !{data.lbl_right_to_opt_out_of_the_sale.en}
                    li !{data.lbl_right_to_non_discrimination.en}
                  p !{data.lbl_to_exercise_your_rights_to_know_or_delete.en}
                  p #{data.lbl_your_request_to_know_or_delete_must_provide.en}
                  
                  p(style='text-align: center;') <u>!{data.lbl_california_residents.en}</u>
