                  //- p #{data.lbl_if_you_are_receiving_promotional_texts_from_instaprotek.en}

                  p #{data.lbl_instaProtek_us_we_or_instaProtek_is_committed.en}
                  p #{data.lbl_we_do_not_knowingly_or_intentionally_attempt_to_solicit.en}
                  p !{data.lbl_what_this_privacy_policy_covers.en}
                  p #{data.lbl_this_privacy_policy_describes_our_practices_in_connection.en}
                  ul
                    li #{data.lbl_through_the_publicly_accessible_websites_we_operate.en}
                    li #{data.lbl_through_our_mobile_apps.en}
                    li #{data.lbl_through_our_social_media_pages_on_platforms.en}
                    li #{data.lbl_through_emails_we_send.en}
                    li #{data.lbl_that_relates_to_our_customers_users_of_our_services.en}
                  p #{data.lbl_collectively_we_refer_to_the_above_mentioned_as_our_service.en}
                  p #{data.lbl_this_privacy_policy_does_not_apply_to_the_processing.en}
                  p #{data.lbl_we_need_to_collect_personal_information.en}
                  p !{data.lbl_what_is_personal_information.en}
                  p #{data.lbl_as_used_in_this_privacy_policy_personal_information.en}
                  
                  p !{data.lbl_the_types_of_personal_information_we_collect.en}
                  p #{data.lbl_we_may_collect_the_following_categories.en}
