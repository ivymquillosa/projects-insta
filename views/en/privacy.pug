                    li !{data.lbl_contact_us_via_email_or_phone_to_communicate_with_us.en}
                    li !{data.lbl_engage_with_our_services_when_you_use_our_apps.en}
                    li !{data.lbl_provide_your_contact_information_on_a_form_and_submit_it_to_us.en}
                    li !{data.lbl_connect_with_our_social_media_pages.en}
                    li !{data.lbl_submit_a_survey_response_or_provide_a_review_of_our_services.en}

                  p <u>#{data.lbl_personal_information_we_collect_automatically.en}</u>
                  p #{data.lbl_when_you_visit_websites_we_and_our_service.en}
                  ul
                    li #{data.lbl_device_information_including_the_type_of_device.en}
                    li #{data.lbl_data_related_to_the_use_our_websites.en}

                  p <u>#{data.lbl_personal_information_we_collect_from_other_sources.en}</u>
                  p #{data.lbl_on_occasion_instaProtek_receives_personal_information.en}
                  ul
                    li #{data.lbl_publicly_available_databases_and_social_media_sites.en}
                    li #{data.lbl_our_advertising_and_marketing_partners.en}
                    li #{data.lbl_when_you_pay_with_a_payment_card.en}

                  p !{data.lbl_cookies_and_other_tracking_technologies.en}
                  p #{data.lbl_we_use_cookies_which_are_pieces_of_information.en}
                  p #{data.lbl_you_have_a_choice_about_the_placement_of_cookies.en}
                  
                  p #{data.lbl_you_can_also_find_additional_information_on_cookie_controls_and_advertisement_here.en}
                  ul
                    li !{data.lbl_network_advertising_initiative.en}
                    li !{data.lbl_digital_advertising_alliance.en}
                    li !{data.lbl_google_allows_users_to_opt_out_of_tracking_by_google_analytics.en}
                  
                  p !{data.lbl_user_of_personal_information.en}
                  p #{data.lbl_we_and_our_services_providers_use_personal_information.en}
                  ul
                    li #{data.lbl_providing_your_our_services.en}
                      ul
                        li #{data.lbl_to_provide_our_websites_and_engage.en}
                        li #{data.lbl_to_provide_our_customer_service.en}
                        li #{data.lbl_to_respond_to_your_inquiries.en}
                        li #{data.lbl_to_send_administrative_information.en}
                    li #{data.lbl_providing_you_with_marketing_materials.en}
                    li #{data.lbl_ensuring_our_services_are_safe.en}
                    li #{data.lbl_analysis_of_personal_information_for_business.en}
                      ul
                        li #{data.lbl_to_analyze_our_customers_and_potential_customers.en}
                        li #{data.lbl_to_better_understand_your_preferences_so_that_we_can_deliver_content.en}
                        li #{data.lbl_to_better_understand_your_preferences_so_that_we_can_personalize_our_interactions.en}
                    li #{data.lbl_for_our_business_operations_purposes_including.en}
                      ul
                        li #{data.lbl_for_developing_new_products_and_services.en}
                        li #{data.lbl_for_enhancing_improving_or_modifying.en}
                        li #{data.lbl_for_determining_the_effectiveness_of_our_promotional_campaigns.en}
                        li #{data.lbl_for_identifying_usage_trends.en}
                        li #{data.lbl_for_operating_and_expanding_our_business_activities.en}

                  p !{data.lbl_how_we_share_information_with_others.en}
                  p #{data.lbl_we_disclose_personal_information.en}
                  ul
                    li #{data.lbl_with_our_service_providers.en}
                      ul
                        li #{data.lbl_data_processing_contractors_that_provide.en}
                        li #{data.lbl_web_hosting_or_web_development_firms.en}
                        li #{data.lbl_our_customer_service_fulfillment.en}
                        li #{data.lbl_billing_providers_that_process_payment.en}
                        li #{data.lbl_mailing_firms_that_will_assist.en}
                        li #{data.lbl_marketing_firms_that_will_assist.en}
                        li #{data.lbl_data_analytics_firms.en}
                    li #{data.lbl_with_our_third_party_marketing_and_advertising.en}
                    li #{data.lbl_with_relevant_business_partners.en}
                      ul
                        li #{data.lbl_with_whom_we_jointly_offer_products.en}
                        li #{data.lbl_to_facilitate_a_direct_relationship.en}
                        li #{data.lbl_to_enable_electronic_communications.en}
                    li #{data.lbl_with_a_purchaser_of_instaProtek.en}
                    li #{data.lbl_to_comply_with_legal_orders.en}
                    li #{data.lbl_to_combat_fraud_or_criminal_activity.en}
                    li #{data.lbl_in_response_to_a_subpoena_or_similar_legal_process.en}
                  
                  p !{data.lbl_your_choices_regarding_your_personal_information.en}
                  p #{data.lbl_we_give_you_choices_regarding_our_use.en}
                  ul
                    li !{data.lbl_receiving_electronic_communications_from_us.en}
                    li !{data.lbl_opt_out_of_text_message_marketing.en}
                    li !{data.lbl_google_analytics_we_use_googles_website.en}
                  p !{data.lbl_how_we_secure_your_data.en}
                  p #{data.lbl_we_use_appropriate_security_measures.en}
                  p !{data.lbl_how_you_can_access_change_or_request_deletion.en}
                  p !{data.lbl_if_you_would_like_to_request_to_access_correct.en}
                  p #{data.lbl_for_your_protection_we_may_only_implement_requests.en}
                  p !{data.lbl_third_party_links.en}
                  p #{data.lbl_our_services_may_from_time_to_time_contain_links.en}
                  p !{data.lbl_california_residents.en}
                  p #{data.lbl_residents_from_california_should_read.en}
                  p !{data.lbl_international_users.en}
                  p #{data.lbl_your_personal_information_will_be_collected_and_stored.en}
                  p !{data.lbl_do_not_track_disclosure.en}
                  p #{data.lbl_regulatory_agencies_such_as_the_u_s_ederal_trade.en}
                  p !{data.lbl_changes_to_our_privacy_policy.en}
                  p #{data.lbl_the_last_updated_legend_at_the_top.en}
                  p !{data.lbl_contact_information_for_complaints_or_concerns.en}
                  p #{data.lbl_if_you_have_any_questions_about_this_policy.en}
                  p
                    a(href='//www.instaprotek.com') https://www.instaprotek.com
                    |
                    span.breakline 1 Wrigley
                    |
                    span.breakline Irvine, CA 92618
                    |
                    span.breakline U.S.A
                    | 
                    a(href='mailto:support@instaprotek.com') support@instaprotek.com
                  
                  h3(style='text-align: center;') #{data.lbl_instaProtek_privacy_policy_for_california_residents.en}
                  //- p !{data.lbl_last_updated.en}
                  p !{data.lbl_this_privacy_policy_for_california_residents_supplements.en}

                  p(style='text-align: center;') <u>!{data.lbl_information_we_collect.en}</u>
                  p !{data.lbl_we_collect_information_that_identifies_relates.en}
                  p #{data.lbl_publicly_available_information_from_government.en}
                  p #{data.lbl_deidentified_or_aggregated_consumer_information.en}
                  p #{data.lbl_in_particular_we_have_collected_the_following_categories.en}
                  
                  table
                    tr
                      th !{data.lbl_table_header_category.en}
                      th !{data.lbl_table_header_examples.en}
                      th !{data.lbl_table_header_collected.en}
                    tr
                      td #{data.lbl_table_a_identifiers.en}
                      td #{data.lbl_table_a_real_name_alias_postal_address.en}
                      td #{data.lbl_table_yes.en}
                    tr
                      td #{data.lbl_table_b_personal_information.en}
                      td 
                        p #{data.lbl_table_a_name_signature_social_security_number.en}
                        |
                        p #{data.lbl_table_some_personal_information.en}
                      td #{data.lbl_table_yes.en}
                    tr
                      td #{data.lbl_table_c_protected_classification.en}
                      td #{data.lbl_table_age_40_years_or_older_race_color.en}
                      td #{data.lbl_table_yes.en}
                    tr
                      td #{data.lbl_table_d_commercial_information.en}
                      td #{data.lbl_table_records_of_personal_property.en}
                      td #{data.lbl_table_no.en}
                    tr
                      td #{data.lbl_table_e_biometric_information.en}
                      td #{data.lbl_table_genetic_physiological_behavioral.en}
                      td #{data.lbl_table_yes.en}
                    tr
                      td #{data.lbl_table_f_internet_or_other.en}
                      td #{data.lbl_table_browsing_history_search_history.en}
                      td #{data.lbl_table_no.en}
                    tr
                      td #{data.lbl_table_g_geolocation_data.en}
                      td #{data.lbl_table_physical_location_or_movements.en}
                      td #{data.lbl_table_no.en}
                    tr
                      td #{data.lbl_table_h_sensory_data.en}
                      td #{data.lbl_table_audio_electronic.en}
                      td #{data.lbl_table_no.en}
                    tr
                      td #{data.lbl_table_i_professional_or_employment.en}
                      td #{data.lbl_table_current_or_past_job.en}
                      td #{data.lbl_table_no.en}
                    tr
                      td #{data.lbl_table_j_non_public_education_information.en}
                      td #{data.lbl_table_education_records_directly.en}
                      td #{data.lbl_table_no.en}
                    tr
                      td #{data.lbl_table_k_Inferences_drawn.en}
                      td #{data.lbl_table_profile_reflecting_a_person.en}
                      td #{data.lbl_table_yes.en}

