                  //-   li #{data.lbl_we_may_use_your_information_to_administer_our_websites.en}

                  //- p #{data.lbl_instaprotek_maintains_certain_protocols_in_order_to_protect_the_security.en}

                  //- ul
                  //-   li #{data.lbl_we_communicate_our_privacy_policy_and_security_guidelines_to_instaprotek_employees.en}
                  //-   li !{data.lbl_we_utilize_secure_socket_layer.en}
