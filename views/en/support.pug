extends layoutfaq

block content
    .section.section__documenthero
      .section--wrapper
        .bgimage.antimoderate.antimoderate-bg(data-hdsrc='/img/getstarted_bg.webp' alt='Instaprotek Background', data-antimoderate-scale='1', data-ldsrc='/img/getstarted_bg_alt.webp', classnames='bgimage')

    .section__group.section__group--offset.bg--darkgray

      section.section.section__documents
        .section--wrapper
          .row
            .col.s12.m12
              .step__doc.anim(data-anim='fadeInDown', data-delay='0s', data-duration='1s')
                h1.step__doc__label #{title}
                .step__doc__form
                  .faq
                    .faq__search
                      .faq__search__wrapper
                        .faq__search__icon
                          i.material-icons search
                        .faq__search__field
                          input.faq__search__input(type='text', placeholder=data.txt_search.en)
                        a.faq__search__clear(href='#clear')
                          i.material-icons clear
                    .faq__page
                      .emptyLabel Sorry, no results found...
                        |
                        a.noresultsfoundclear(href='#clear') Click here to clear search query.
                      .faq__group
                        h3.faq__group__title #{data.lbl_top_faqs.en}
                        ol.faq__list
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_how_do_i_register_my_device.en}
                            .faq__list__item--answer
                              p #{data.lbl_get_started_with_insta.en}
                              p #{data.lbl_scan_qr_code.en}
                              p #{data.lbl_download_app_manually.en}
                              a.faq__store__link(href='https://apps.apple.com/us/app/instaprotek/id1456989327', target='_blank', title='Download on the Apple Store')
                                img(src='/img/download_apple_store.webp' alt='Apple brand logo')
                              a.faq__store__link(href='https://play.google.com/store/apps/details?id=com.instaprotek.app&hl=en_US', target='_blank', title='Get it on Google Play') 
                                img(src='/img/download_google_play.webp' alt='Google play logo')

                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_what_is_the_step_by_step_process.en} 
                            .faq__list__item--answer
                              p #{data.lbl_registration_is_easy.en}
                              .faq__video
                                p.video_header #{data.lbl_android.en}
                                video(id='my-video', class='video-js', controls, preload='auto', data-setup='{"aspectRatio":"16:9"}', poster="https://acdn.dnamicro.net/instaprotek/Instaprotek_registration_thumbnail.png")
                                  source(src="https://acdn.dnamicro.net/instaprotek/others/FINAL_Instaprotek_android_registration_June_2023.mp4", type='video/mp4')
                                  p(class='vjs-no-js') To view this video please enable JavaScript, and consider upgrading to a web browser that
                              .faq__video
                                p.video_header #{data.lbl_ios.en}
                                video(id='my-video', class='video-js', controls, preload='auto', data-setup='{"aspectRatio":"16:9"}', poster="https://acdn.dnamicro.net/instaprotek/Instaprotek_registration_thumbnail.png")
                                  source(src="https://acdn.dnamicro.net/instaprotek/FINAL_Instaprotek_iOS_Registration_June_2023.mp4", type='video/mp4')
                                  p(class='vjs-no-js') To view this video please enable JavaScript, and consider upgrading to a web browser that
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_how_can_i_register_my_product.en}
                            .faq__list__item--answer
                              p #{data.lbl_register_within_30days.en}
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_how_long_does_my_limited_warranty_last.en}
                            .faq__list__item--answer
                              p #{data.lbl_lasts_for_1_year_from_date_purchase.en}
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_how_can_i_file_a_claim.en}
                            .faq__list__item--answer
                              p !{data.lbl_easily_file_a_claim_using_the_app.en}
                              .faq__video
                                p.video_header #{data.lbl_android.en}
                                video(id='my-video', class='video-js', controls, preload='auto', data-setup='{"aspectRatio":"16:9"}', poster="https://acdn.dnamicro.net/instaprotek/Instaprotek-HowtoSelfClaim_thumbnail.png")
                                  source(src="https://acdn.dnamicro.net/instaprotek/Instaprotek-HowtoSelfClaim_android_July-25-2023.mp4", type='video/mp4')
                                  p(class='vjs-no-js') To view this video please enable JavaScript, and consider upgrading to a web browser that
                              .faq__video
                                p.video_header #{data.lbl_ios.en}
                                video(id='my-video', class='video-js', controls, preload='auto', data-setup='{"aspectRatio":"16:9"}', poster="https://acdn.dnamicro.net/instaprotek/Instaprotek-HowtoSelfClaim_thumbnail.png")
                                  source(src="https://acdn.dnamicro.net/instaprotek/FINAL_Instaprotek_iOS_Claim_June_2023.mp4", type='video/mp4')
                                  p(class='vjs-no-js') To view this video please enable JavaScript, and consider upgrading to a web browser that
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_can_i_file_a_claim_after_my_device.en}
                            .faq__list__item--answer
                              p !{data.lbl_before_proceeding_with_the_repair.en}
                          
                      .faq__group
                        h3.faq__group__title #{data.lbl_warranty_and_replacement.en}
                        ol.faq__list
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_how_long_does_my_limited_warranty_last.en}
                            .faq__list__item--answer
                              p #{data.lbl_limited_warranty_for_your_product.en}
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_do_you_cover_the_replacement_of_the_screen.en}
                            .faq__list__item--answer
                              p #{data.lbl_yes_if_your_product_comes.en}
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_how_will_i_know_if_im_eligible_to_file_a_claim.en}
                            .faq__list__item--answer
                              p #{data.lbl_to_determine_your_eligibility.en}
                          

                      .faq__group
                        h3.faq__group__title #{data.lbl_registration.en}
                        ol.faq__list
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_how_do_i_register_my_device.en}
                            .faq__list__item--answer
                              p #{data.lbl_get_started_with_insta.en}
                              p #{data.lbl_scan_qr_code.en}
                              p #{data.lbl_download_app_manually.en}
                              a.faq__store__link(href='https://apps.apple.com/us/app/instaprotek/id1456989327', target='_blank', title='Download on the Apple Store')
