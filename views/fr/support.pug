extends layoutfaq

block content
    .section.section__documenthero
      .section--wrapper
        .bgimage.antimoderate.antimoderate-bg(data-hdsrc='/img/getstarted_bg.webp' alt='Instaprotek Background', data-antimoderate-scale='1', data-ldsrc='/imgs/getstarted_bg_alt.png', classnames='bgimage')

    .section__group.section__group--offset.bg--darkgray

      section.section.section__documents
        .section--wrapper
          .row
            .col.s12.m12
              .step__doc.anim(data-anim='fadeInDown', data-delay='0s', data-duration='1s')
                h1.step__doc__label #{title}
                .step__doc__form
                  .faq
                    .faq__search
                      .faq__search__wrapper
                        .faq__search__icon
                          i.material-icons search
                        .faq__search__field
                          input.faq__search__input(type='text', placeholder=data.txt_search.fr)
                        a.faq__search__clear(href='#clear')
                          i.material-icons clear
                    .faq__page
                      .emptyLabel #{data.err_txt_sorry_no_result_found.fr}
                        |
                        a.noresultsfoundclear(href='#clear') #{data.err_txt_click_here_to_clear.fr}
                      .faq__group
                        h3.faq__group__title #{data.lbl_top_faqs.fr}
                        ol.faq__list
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_how_do_i_register_my_device.fr}
                            .faq__list__item--answer
                              p #{data.lbl_download_our_app_app_store_google_playstore.fr}
                              a.faq__store__link(href='https://apps.apple.com/us/app/instaprotek/id1456989327', target='_blank', title='Download on the Apple Store')
                                img(src='/img/download_app_store_fr.webp' alt='Apple brand logo')
                              a.faq__store__link(href='https://play.google.com/store/apps/details?id=com.instaprotek.app&hl=en_US', target='_blank', title='Get it on Google Play') 
                                img(src='/img/download_google_play_fr.webp' alt='Get it on Google Play in french')

                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_what_is_the_step_by_step_process.fr} 
                            .faq__list__item--answer
                              p #{data.lbl_registration_is_easy_on_the_app.fr}
                              .faq__video
                                p.video_header #{data.lbl_android.fr}
                                video(id='my-video', class='video-js', controls, preload='auto', data-setup='{"aspectRatio":"16:9"}', poster="https://acdn.dnamicro.net/instaprotek/android_poster_registration_eng.png")
                                  source(src="https://acdn.dnamicro.net/instaprotek/registration_eng.mp4", type='video/mp4')
                                  p(class='vjs-no-js') To view this video please enable JavaScript, and consider upgrading to a web browser that
                              .faq__video
                                p.video_header #{data.lbl_ios.fr}
                                video(id='my-video', class='video-js', controls, preload='auto', data-setup='{"aspectRatio":"16:9"}', poster="https://acdn.dnamicro.net/instaprotek/iOS_poster_registration_eng.png")
                                  source(src="https://acdn.dnamicro.net/instaprotek/registration_iOS.mp4", type='video/mp4')
                                  p(class='vjs-no-js') To view this video please enable JavaScript, and consider upgrading to a web browser that
                          li.faq__list__item
                            a.faq__list__item--question(href='#faq') #{data.lbl_how_long_does_my_limited_warranty_last.fr}
                            .faq__list__item--answer
