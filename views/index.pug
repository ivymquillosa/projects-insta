extends layout

block content

    section.section.section__hero
      .section--wrapper
        .slider
          .slide-item
            .slide-captions2
              .slide-context
                h1.section__title.hide-mobile
                  span.anim.reanim(data-anim='fadeInUp', data-delay='1s', data-duration='1s') #{data.lbl_set_your_product_apart_using_an_itelligent_product.en}
                  //- span.anim.reanim(data-anim='fadeInUp', data-delay='1.3s', data-duration='1s') bbbb
                  //- span.anim.reanim(data-anim='fadeInUp', data-delay='1.6s', data-duration='1s') abbbbaa
                p.section__subtitle.anim.reanim.hide-mobile(data-anim='fadeInUp', data-delay='1.9s', data-duration='1s')
                  span #{data.lbl_a_low_cost_and_easy_to_use_prodiuct_guarantee.en}

                .show-mobile.anim.reanim(data-anim='fadeInUp', data-delay='0s', data-duration='1s')
                  p.section__title
                    | #{data.lbl_set_your_product_apart_using_an_itelligent_product.en}
                  p.section__subtitle
                    | #{data.lbl_a_low_cost_and_easy_to_use_prodiuct_guarantee.en}

                .hero__buttons
                  button.waves-effect.waves-light.btn.calltoaction.calltoaction--primary.anim.reanim.modal-trigger(data-target='modalGetStarted', data-anim='fadeInDown', data-delay='3s', data-duration='1s', href='#getstartednow')
                    span.btn__text #{data.btn_get_started_now.en}

              .slide-video.anim.reanim(data-anim='fadeInRight', data-delay='1s', data-duration='1s')
                video(id='my-video', class='video-js', controls, preload='auto', data-setup='{"aspectRatio":"16:9"}', poster='./img/warranty/video_poster.webp')
                  source(src="https://acdn.dnamicro.net/instaprotek/instaprotek_video.mp4", type='video/mp4')
                  p(class='vjs-no-js') To view this video please enable JavaScript, and consider upgrading to a web browser that
            .bgimage.tint2.antimoderate.antimoderate-bg(data-hdsrc='/img/warranty/Hero2v.1.webp' alt='Instaprotek hero lady holding her phone', data-antimoderate-scale='1', data-ldsrc='/img/warranty/Hero2_alt.webp', classnames='bgimage tint2')

    section.section.section__1.section--withbg
      .section--wrapper
        .row
          .col.s12.m6.subsection1.anim(data-anim='zoomIn', data-delay='0s', data-duration='1s')
            .device__box--warranty
              .download__phoneimg1
                img(src='/img/warranty/device_1_alt.webp').antimoderate(data-hdsrc='/img/warranty/device_1.webp'  alt='Instaprotek App Register Product page displaying on Smartphones screen', data-antimoderate-scale='1', ldsrc='/img/warranty/device_1_alt.webp')
              .download__phoneimg2
                img(src='/img/warranty/device_2_alt.webp').antimoderate(data-hdsrc='/img/warranty/device_2.webp' alt="Instaprotek App Device Registration page displaying on a Smartphone's screen", data-antimoderate-scale='1', ldsrc='/img/warranty/device_2_alt.webp')

          .col.s12.m6.subsection2
            p.section__title.hide-mobile.anim(data-anim='fadeInRight', data-delay='0.3s', data-duration='1s')
              span #{data.lbl_frictionless_app_driven_registration.en}
            .show-mobile.anim(data-anim='fadeInUp', data-delay='0s', data-duration='1s')
              p.section__title #{data.lbl_frictionless_app_driven_registration.en}

      .bgimage.antimoderate.antimoderate-bg(data-hdsrc='/img/warranty/bg_1.webp', data-antimoderate-scale='1', data-ldsrc='/img/warranty/bg_1.webp', classnames='bgimage')

    section.section.section__2.section--withbg
      .section--wrapper
        .row
          .col.s12.m6.subsection1
            p.hide-mobile.section__title.anim(data-anim='fadeInLeft', data-delay='0s', data-duration='1s')
              span #{data.lbl_smart_and_modern_platform.en}
            .show-mobile.anim(data-anim='fadeInUp', data-delay='0s', data-duration='1s')
              p.section__title #{data.lbl_smart_and_modern_platform.en}

          .col.s12.m6.subsection2.anim(data-anim='fadeInRight', data-delay='0.3s', data-duration='1s')
            img(src='/img/warranty/2_alt.webp').antimoderate(data-hdsrc='/img/warranty/2.webp' alt='Instaprotek CRM PortalProduct page and send Notification page overlapping each other', data-antimoderate-scale='1', ldsrc='/img/warranty/2_alt.webp')

    section.section.section__3
      .section--wrapper
        .row
          .col.s12.m4.subsection1.anim(data-anim='fadeInLeft', data-delay='0s', data-duration='1s')
            img(src='/img/warranty/anti-fraud_alt.webp').antimoderate(data-hdsrc='/img/warranty/anti-fraud.webp' alt='No Fraudster sign next to a Smartphone with security check icon', data-antimoderate-scale='1', ldsrc='/img/warranty/anti-fraud_alt.webp')

          .col.s12.m8.subsection2
            p.hide-mobile.section__title.anim(data-anim='fadeInRight', data-delay='0.3s', data-duration='1s')
              span #{data.lbl_reduce_fruad_through_efficient_app.en}
            .show-mobile.anim(data-anim='fadeInUp', data-delay='0s', data-duration='1s')
              p.section__title #{data.lbl_reduce_fruad_through_efficient_app.en}

    section.section.section__4.section--withbg
      .section--wrapper
        .row
          .col.s12.m6.subsection1
            p.hide-mobile.section__title.anim(data-anim='fadeInLeft', data-delay='0s', data-duration='1s')
              span #{data.lbl_full_access_to_customizable_analytics.en}
            .show-mobile.anim(data-anim='fadeInUp', data-delay='0s', data-duration='1s')
              p.section__title #{data.lbl_full_access_to_customizable_analytics.en}

          .col.s12.m6.subsection2.anim(data-anim='fadeInRight', data-delay='0.3s', data-duration='1s')
            img(src='/img/warranty/4_alt.webp').antimoderate(data-hdsrc='/img/warranty/4.webp' alt='Registration by Device Bar Chart overlapping with App Installs Wave Chart', data-antimoderate-scale='1', ldsrc='/img/warranty/4_alt.webp')

    section.section.section__5
      .section--wrapper
          .hide-mobile.section__title.anim(data-anim='fadeInUp', data-delay='0s', data-duration='1s')
            span #{data.lbl_increase_your_revenue_even_more.en} 
            //- a(href='//register.instaprotek.com') #{data.btn_registration_app.en}
          .show-mobile.section__title.anim(data-anim='fadeInUp', data-delay='0s', data-duration='1s') #{data.lbl_increase_your_revenue_even_more.en} 
            //- | 
            //- a(href='//register.instaprotek.com') #{data.btn_registration_app.en}


          .section_image.anim(data-anim='fadeInDown', data-delay='0.5s', data-duration='1s')
            img(src='/img/warranty/last_alt.webp').antimoderate(data-hdsrc='/img/warranty/last.webp' alt='Screen of two Smartphones displaying products for sale and product protection plan', data-antimoderate-scale='1', ldsrc='/img/warranty/last_alt.webp')

    section.section.section__getstarted
      .section--wrapper
        .row
          p.section__title.anim.reanim(data-anim='fadeInUp', data-delay='0s', data-duration='1s') #{data.lbl_contact_us_to_learn_how_instaprotek.en}
          a.waves-effect.waves-light.btn.calltoaction.calltoaction--secondary.anim.reanim.modal-trigger(href='#getstartednow', data-target='modalGetStarted', data-anim='fadeInDown', data-delay='0.8s', data-duration='1s') #{data.btn_get_started_now.en}
      .bgimage.antimoderate.antimoderate-bg(data-hdsrc='/img/getstarted_bg.webp' alt='Instaprotek Background', data-antimoderate-scale='1', data-ldsrc='/img/getstarted_bg_alt.webp', classnames='bgimage')